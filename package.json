{
  "name": "react-modele",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.dev.js --open",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint --fix src/**/*.js",
    "clean": "rm -rf dist",
    "clean:all": "rm -rf dist node_modules yarn.lock package-lock.json",
    "test": "cross-env NODE_PATH=./ mocha --require tests/.setup.js tests/**/*.test.js",
    "test:watch": "cross-env NODE_PATH=./ mocha --watch --require tests/.setup.js tests/**/*.test.js"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "*",
    "@babel/plugin-proposal-object-rest-spread": "*",
    "@babel/preset-env": "*",
    "@babel/preset-react": "*",
    "@babel/register": "*",
    "babel-eslint": "*",
    "babel-loader": "*",
    "bundle-stats-webpack-plugin": "*",
    "chai": "*",
    "clean-webpack-plugin": "*",
    "copy-webpack-plugin": "^6.2.0",
    "cross-env": "*",
    "css-loader": "^4.3.0",
    "cssnano": "*",
    "enzyme": "*",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.10.0",
    "eslint-config-airbnb": "*",
    "eslint-import-resolver-alias": "*",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "file-loader": "*",
    "html-webpack-plugin": "^4.5.0",
    "ignore-styles": "*",
    "jsdom": "*",
    "mini-css-extract-plugin": "^0.11.3",
    "mocha": "*",
    "optimize-css-assets-webpack-plugin": "*",
    "postcss-loader": "^4.0.3",
    "postcss-preset-env": "*",
    "resolve-url-loader": "*",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.3.0",
    "terser-webpack-plugin": "^4.2.2",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^5.1.4"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "prop-types": "*",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.0"
  }
